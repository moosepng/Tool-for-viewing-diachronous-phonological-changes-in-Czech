    {
        "id": "prehlaska-mekcici-ui",
        "period": 1,
        "periodName": "Čeština 14. století",
        "name": "Přehláska měkčícího 'u > i",
        "patterns": [
            {
                "search": "([žščřcjďťň])u",
                "replace": "$1i"
            },
            {
                "search": "([žščřcjďťň])ú",
                "replace": "$1í"
            },
            {
                "search": "'ú",
                "replace": "í"
            },
            {
                "search": "'u",
                "replace": "i"
            }
        ]
    },
